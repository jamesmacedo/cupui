{
    "name": "cupui-json",
    "version": "0.0.1",
    "description": "What about format your raw JSON data to an formatted output easily? Cupui implements a schema system to help you format your json output",
    "type": "module",
    "files": [
        "dist"
    ],
    "main": "dist/cupui.es.js",
    "module": "dist/cupui.cjs.js",
    "types": "dist/main.d.js",
    "scripts": {
        "build": "rollup -c rollup.config.js",
        "watch": "rollup --watch -c rollup.config.js",
        "test":"jest tests/",
        "test:watch":"jest --watchAll"
    },
    "exports": {
        ".": {
            "require": "./dist/cupui.cjs.js",
            "import": "./dist/cupui.es.js"
        }
    },
    "dependencies": {
        "@rollup/plugin-typescript": "^11.1.6",
        "rollup": "^4.14.2",
        "typescript": "^5.4.5"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^25.0.7",
        "jest": "^29.7.0",
        "tslib": "^2.6.2"
    }
}
